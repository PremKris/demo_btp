{
  "_version": "1.59.0",
  "sap.app": {
    "id": "compositionassociation",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.12.3",
      "toolsId": "a8d4168a-5e07-41c8-bed8-e5e24816dd60"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/service1/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.6",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "compositionassociation.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "style/style.css"
        }
      ]
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "schoolList",
          "target": "schoolList"
        },
        {
          "pattern": "school({key}):?query:",
          "name": "schoolObjectPage",
          "target": "schoolObjectPage"
        }
      ],
      "targets": {
        "schoolList": {
          "type": "Component",
          "id": "schoolList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/school",
              "variantManagement": "Page",
              "navigation": {
                "school": {
                  "detail": {
                    "route": "schoolObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "quickVariantSelection": {
                      "hideTableTitle": true
                    },
                    "selectionLimit": 2,
                    "selectionMode": "None"
                  }
                }
              },
              "initialLoad": "Enabled",
              "content": {
                "header": {
                  "actions": {
                    "1": {
                      "press": "compositionassociation.ext.controller.To.Tomethod",
                      "visible": true,
                      "enabled": true,
                      "text": "Capire"
                    }
                  }
                }
              }
            }
          }
        },
        "schoolObjectPage": {
          "type": "Component",
          "id": "schoolObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/school"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
